{
  "$id": "input-create-users.json#",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["email", "password"],
  "minProperties": 1,
  "properties": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string"
    },
    "passwordConfirm": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
