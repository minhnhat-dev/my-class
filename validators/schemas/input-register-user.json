{
  "$id": "input-register-user.json#",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [],
  "minProperties": 1,
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minimum": 8,
      "passwordChecker": true
    },
    "roles": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}
